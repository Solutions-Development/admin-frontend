  <div class="card" style="padding: 20px;">
                      <form>
                        <h4 style="text-align: center;">Patrocinadores</h4>
                        <hr>
                        <div class="form-row">
                          <div class="form-group col-md-6">
                            <label>Subir Imagen</label>
                            <input class="btn" type='file' id="sponsor1" name="diamanteImagen" value=`this.src`/>
                            <img src="https://placehold.it/920x200" class="img-thumbnail" id="img_sponsor1" alt="img_sponsor1" width="200" height="200">
                          </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label>Nombre del Patrocinador</label>
                            <input required type="text" name="diamanteNombre" class="form-control" placeholder="Laboratorios Palo Alto">
                          </div>
                         <div class="form-group col-md-6">
                            <label>Tipo de Patrocinador</label>
                            <input required type="text" name="tipo" class="form-control" placeholder="Patrocinador Diamante">
                          </div>
                      </form>
                    </div>
